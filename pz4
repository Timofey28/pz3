#include <bits/stdc++.h>
using namespace std;

bool isValid(string s)
{
    map<char, char> brackets = {{'(', ')'}, {'[', ']'}, {'{', '}'}};
    stack<char> last_bracket;
    for(const auto &x : s) {
        if(x == '(' || x == '[' || x == '{') last_bracket.push(brackets.find(x)->second);
        else if(x == ')' || x == ']' || x == '}') {
            if(!last_bracket.size() || x != last_bracket.top()) return 0;
            last_bracket.pop();
        }
    }
    return 1;
}

int main()
{
    string s = "qwe(zxxcx[213asdc]xcz)sx{zxc}cfg";
    cout << boolalpha << isValid(s) << "\n"; // true

    return 0;
}
